{
    "name": "mnapoli/doctrine-translated",
    "description": "Translated strings for Doctrine",
    "keywords": ["doctrine", "translatable", "translation", "translations", "i18n"],
    "homepage": "https://github.com/mnapoli/DoctrineTranslated",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Mnapoli\\Translated\\": "src/",
            "Test\\Mnapoli\\Translated\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "doctrine/orm": "2.5.*@dev"
    },
    "require-dev": {
        "twig/twig": "~1.15"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
